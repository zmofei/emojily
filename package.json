        "transfer",
        "convert"
    ],
    "author": "Mofei Zhu <https://hiMofei.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/zmofei/emojily/issues"
    },
    "homepage": "https://github.com/zmofei/emojily#readme",
    "devDependencies": {
        "codecov": "^3.1.0",
        "nyc": "^13.0.1",
        "opener": "^1.5.1",
        "tap-spec": "^5.0.0",
        "tape": "^4.9.1",
        "webpack": "^4.19.1",
        "webpack-cli": "^3.1.0"
    }
}
